	&lt;key&gt;AuthorName&lt;/key&gt;
	&lt;string&gt;&lt;/string&gt;
	&lt;key&gt;AuthorEmail&lt;/key&gt;
	&lt;string&gt;&lt;/string&gt;
&lt;/dict&gt;
</string>
		<key>main.m</key>
		<dict>
			<key>Path</key>
			<string>main.m</string>
		</dict>
		<key>Hook/CaptainHook.h</key>
		<dict>
			<key>Path</key>
			<string>CaptainHook.h</string>
			<key>Group</key>
			<string>Hook</string>
		</dict>
	</dict>
