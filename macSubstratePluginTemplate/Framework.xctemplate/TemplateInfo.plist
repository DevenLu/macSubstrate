			<true/>
			<key>NotPersisted</key>
			<true/>
			<key>Type</key>
			<string>text</string>
			<key>Default</key>
			<string></string>
		</dict>
	</array>
	<key>Nodes</key>
	<array>
		<string>Info.plist:macSubstratePlugin</string>
		<string>main.m</string>
		<string>Hook/CaptainHook.h</string>
	</array>
	<key>Definitions</key>
	<dict>
		<key>___PACKAGENAME___.h</key>
		<dict>
			<key>TargetIdentifiers</key>
			<array>
				<string>com.apple.dt.cocoaLibraryOrFrameworkTarget</string>
			</array>
			<key>BuildAttributes</key>
			<array>
				<string>Public</string>
			</array>
